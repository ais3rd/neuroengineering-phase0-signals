import matplotlib.pyplot as plt
import numpy as np

# Buat array 1000 titik dari 0 sampai 2
x = np.linspace(0,2,1000)
f = 1/2 #Frekuensi dibuat 1/2 Hz
y = np.sin(2 * np.pi * f * x)

plt.figure(figsize=(8,4))
plt.plot(x,y)
plt.title("Sine Wave (1/2 Hz)")
plt.xlabel("Time (s)")
plt.ylabel("Amplitude")
plt.grid(True)
plt.show()
